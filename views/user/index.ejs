<h1>Users</h1>

<ul>
  <% users.forEach(function (user) { %>
  <li>
    <span><%= user.name %></span>
    <a href="/user/<%= user.id %>">Show</a>
    <a href="/user/<%= user.id %>/edit">Edit</a>
    
    <a href="/user/<%= user.id %>" class="user-destroy">Destroy</a>

  </li>
  <% }) %>
</ul>

<a href="/user/new">Add user</a>